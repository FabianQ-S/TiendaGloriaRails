<div id="<%= dom_id product %>">
  <div>
    <strong>Sku:</strong>
    <%= product.sku %>
  </div>

  <div>
    <strong>Name:</strong>
    <%= product.name %>
  </div>

  <div>
    <strong>Description:</strong>
    <%= product.description %>
  </div>

  <div>
    <strong>Price:</strong>
    <%= product.price %>
  </div>

  <div>
    <strong>Image url:</strong>
    <%= product.image_url %>
  </div>

  <div>
    <strong>Category:</strong>
    <%= product.category_id %>
  </div>

  <div>
    <strong>Provider:</strong>
    <%= product.provider_id %>
  </div>

  <div>
    <strong>Is perishable:</strong>
    <%= product.is_perishable %>
  </div>

</div>
