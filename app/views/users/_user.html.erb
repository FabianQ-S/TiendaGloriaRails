<div id="<%= dom_id user %>">
  <div>
    <strong>Role:</strong>
    <%= user.role_id %>
  </div>

  <div>
    <strong>Name:</strong>
    <%= user.name %>
  </div>

  <div>
    <strong>Email:</strong>
    <%= user.email %>
  </div>

  <div>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </div>

  <div>
    <strong>Active:</strong>
    <%= user.active %>
  </div>

</div>
